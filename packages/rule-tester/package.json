{
  "name": "@theguild/eslint-rule-tester",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "9.39.3"
  },
  "dependencies": {
    "@babel/code-frame": "^7.18.6"
  },
  "devDependencies": {
    "@types/babel__code-frame": "7.27.0",
    "@types/node": "22.19.13",
    "eslint": "9.39.3"
  }
}
